# Configuración de Firebase
FIREBASE_API_KEY=your-firebase-api-key
FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
FIREBASE_PROJECT_ID=your-project-id
FIREBASE_STORAGE_BUCKET=your-project.appspot.com
FIREBASE_MESSAGING_SENDER_ID=123456789
FIREBASE_APP_ID=your-app-id

# Configuración de AdMob
ADMOB_ANDROID_APP_ID=ca-app-pub-XXXXXXXXXXXXXXXX~XXXXXXXXXX
ADMOB_BANNER_ID=ca-app-pub-XXXXXXXXXXXXXXXX/XXXXXXXXXX
ADMOB_INTERSTITIAL_ID=ca-app-pub-XXXXXXXXXXXXXXXX/XXXXXXXXXX
ADMOB_REWARDED_ID=ca-app-pub-XXXXXXXXXXXXXXXX/XXXXXXXXXX

# Configuración de Google Play Billing
GOOGLE_PLAY_BILLING_KEY=your-billing-key

# Configuración de la App
APP_NAME=MiTratamiento
APP_VERSION=1.0.0
APP_ENVIRONMENT=development

# URLs de la API (si aplica)
API_BASE_URL=https://api.mitratamiento.com
API_TIMEOUT=30000

# Configuración de notificaciones
NOTIFICATION_CHANNEL_ID=mitratamiento_notifications
NOTIFICATION_CHANNEL_NAME=MiTratamiento Notifications

# Configuración de desarrollo
DEBUG_MODE=true
ENABLE_LOGGING=true
ENABLE_ANALYTICS=false
