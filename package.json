{
  "name": "mitratamiento",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "npx expo start",
    "android": "npx expo start --android",
    "ios": "npx expo start --ios",
    "web": "npx expo start --web",
    "install-deps": "node install-dependencies.js",
    "install-windows": "node install-windows.js",
    "fix-deps": "node fix-dependencies.js",
    "verify": "node verificar-instalacion.js",
    "init": "node iniciar-app.js",
    "test": "node probar-app.js",
    "clean": "node -e \"const fs=require('fs'); if(fs.existsSync('node_modules')) fs.rmSync('node_modules',{recursive:true,force:true}); if(fs.existsSync('package-lock.json')) fs.unlinkSync('package-lock.json');\" && npm cache clean --force",
    "reset": "npm run clean && npm install --legacy-peer-deps"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.18.2",
    "@react-navigation/bottom-tabs": "^6.5.8",
